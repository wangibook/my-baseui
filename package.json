{
  "name": "my-baseui",
  "author": "wangdashi",
  "private": false,
  "version": "0.0.17",
  "description": "my-baseui",
  "main": "./dist/my-baseui.umd.js",
  "module": "./dist/my-baseui.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-baseui.es.js",
      "require": "./dist/my-baseui.umd.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite --host",
    "build:docs": "vite build --config ./build/docs.config.js && node ./config/copyDocs.js",
    "build:dist": "vite build --config ./build/dist.config.js",
    "preview": "vite preview",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "highlight.js": "^11.5.1",
    "mitt": "^3.0.0",
    "vite-plugin-md": "^0.12.4",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "copy-dir": "^1.3.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "sass": "^1.52.2",
    "vite": "^2.9.9"
  },
  "license": "ISC"
}
